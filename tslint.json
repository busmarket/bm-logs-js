{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "no-console": false,
        "no-reference": false,
        "variable-name": false,
        "no-empty-interface": false,
        "max-classes-per-file": [true, 5, "exclude-class-expressions"]
    },
    "rulesDirectory": []
}
